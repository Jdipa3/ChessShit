package model;

import java.awt.Color;

import javax.swing.ImageIcon;

public class Rook extends SuperPiece{
	public Rook(Color clr){
		if(clr == Color.BLACK){
			this.pieceIcon = new ImageIcon(getClass().getClassLoader().getResource("black-bishop.png"));
		}else{
			this.pieceIcon = new ImageIcon(getClass().getClassLoader().getResource("white-bishop.png"));
		}
	}

}
